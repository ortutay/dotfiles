<script>
  function createAccount() {
    $("#createform :submit").attr("disabled", "disabled");
    var params = $("#createform").serialize();
    $.ajax({
            type: "POST",
            url: "/ui/signup",
            beforeSend: function(xhr) {
              xhr.setRequestHeader('X-CSRF-Token', $('meta[name="csrf-token"]').attr('content'));
              $("#create_errors").html("");
            },
            data: params,
            dataType: "json",
            success: function(data, status, request) {
              if (!data.ok) {
                $("#createform :submit").removeAttr("disabled");
                var inner =  $(data.status).map(function(i, msg) {return "<li>" + msg + "</li>"})
                inner = inner.toArray()
                inner = $.unique($(inner).toArray()).join("");
                $("#create_errors").html("<div class='alert alert-danger'><ul>" + inner+ "</ul></div>");
                console.log([data, status, request]);
              } else {
                if (data.redirect_url) {
                  window.location = data.redirect_url;
                } else {
                  window.location = '/ui/account';
                }
              }
            },
            error: function(data, status, request) {
              $("#createform :submit").removeAttr("disabled");
              alert("Couldn't sign up due to a server and/or network error.  If this persists please email founders@starfighters.io -- we'll take care of it.");
              console.log(["FAILED AJAX:", data, status, request]);
            }
     })
  }
  
  function signIn() {
   var params = $("#loginform").serialize();
   $("#loginform :submit").attr("disabled", "disabled");
    $.ajax({
            type: "POST",
            url: "/ui/login",
            beforeSend: function(xhr) {
              xhr.setRequestHeader('X-CSRF-Token', $('meta[name="csrf-token"]').attr('content'));
              $("#signin_errors").html("");
            },
            data: params,
            dataType: "json",
            success: function(data, status, request) {
              if (data.redirect_url) {
                window.location = data.redirect_url;
              } else {
                window.location = '/ui/account';
              }
            },
            error: function(data, status, request) {
              $("#loginform :submit").removeAttr("disabled");
              $("#signin_errors").html("<div class='alert alert-danger'>Couldn't sign in. Check username/password.</div>")
            }
     })
  }
</script>

